{
  "name": "jtools",
  "version": "0.0.0",
  "author": "Jawil",
  "scripts": {
    "build": "vue-tsc --noEmit && vite build",
    "dev": "vite",
    "prepare": "husky install",
    "preview": "vite preview"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "**/*": "prettier --write --ignore-unknown",
    "*.{ts,js,vue}": [
      "eslint"
    ]
  },
  "dependencies": {
    "@vueuse/core": "^8.4.2",
    "@vueuse/router": "^8.4.2",
    "compressorjs": "^1.1.1",
    "dayjs": "^1.11.1",
    "lunar-typescript": "^1.2.21",
    "naive-ui": "^2.28.2",
    "normalize.css": "^8.0.1",
    "nprogress": "^0.2.0",
    "nzh": "^1.0.8",
    "vue": "^3.2.33",
    "vue-router": "^4.0.15"
  },
  "devDependencies": {
    "@types/node": "^17.0.31",
    "@types/nprogress": "^0.2.0",
    "@typescript-eslint/eslint-plugin": "^5.22.0",
    "@typescript-eslint/parser": "^5.22.0",
    "@vicons/carbon": "^0.12.0",
    "@vicons/fluent": "^0.12.0",
    "@vitejs/plugin-vue": "^2.3.2",
    "autoprefixer": "^10.4.7",
    "eslint": "^8.14.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-vue": "^8.7.1",
    "husky": "^7.0.4",
    "lint-staged": "^12.4.1",
    "postcss": "^8.4.13",
    "prettier": "^2.6.2",
    "prettier-plugin-tailwindcss": "^0.1.10",
    "tailwindcss": "^3.0.24",
    "typescript": "^4.6.4",
    "unplugin-auto-import": "^0.8.6",
    "unplugin-vue-components": "^0.19.6",
    "vite": "^2.9.8",
    "vue-tsc": "^0.29.8"
  }
}
